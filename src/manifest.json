{
  "name": "Salesforce Lightning to Classic",
  "description": "Redirect Salesforce Lightning case links to Salesforce Classic",
  "author": "Kellen Hawley",
  "version": "1.0.0",
  "icons": {},
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Salesforce Lightning Redirect"
  },
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "*://lightspeed.lightning.force.com/"
  ],
  "manifest_version": 2
}