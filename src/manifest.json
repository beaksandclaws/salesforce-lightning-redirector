{
  "name": "Salesforce Lightning to Classic",
  "description": "Redirect Salesforce Lightning to Salesforce Classic",
  "author": "Kellen Hawley",
  "version": "1.0.2",
  "icons": {
    "16": "assets/salesforce-lightning-redirect-16.png",
    "48": "assets/salesforce-lightning-redirect-48.png",
    "128": "assets/salesforce-lightning-redirect-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Salesforce Lightning Redirect"
  },
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "*://*.lightning.force.com/",
    "*://*.my.salesforce.com/"
  ],
  "manifest_version": 2
}